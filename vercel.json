{
  "version": 2,
  "builds": [
    {
      "src": "**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/contact.php",
      "dest": "/contact.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

